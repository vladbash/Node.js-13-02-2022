<!DOCTYPE html>
<html lang="en">
<%- include('_partials/head.ejs', { title: 'Home' }) %>

<body class="d-flex flex-column vh-100">
    <%- include('_partials/header', { auth }); -%>
    <main class="flex-shrink-0 h-90">
        <div class="container">
            <div
                class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                <h1 class="h2">Documents</h1>
                <div class="btn-toolbar mb-2 mb-md-0">
                    <% if (auth) { %>
                    <div class="btn-group me-2">
                        <a class="btn btn-sm btn-outline-secondary" href="/document/create">Create</a>
                    </div>
                    <% } %>
                </div>
            </div>
            <div class="list-group list-group-checkable d-grid gap-2 border-0 w-auto">
  
                <label class="list-group-item rounded-3 py-3" for="listGroupCheckableRadios1">
                  First radio
                  <span class="d-block small opacity-50">With support text underneath to add more detail</span>
                </label>
              </div>
        </div>
    </main>
    <%- include('_partials/footer'); -%>
</body>

</html>